/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode() {}
 *     TreeNode(int val) { this.val = val; }
 *     TreeNode(int val, TreeNode left, TreeNode right) {
 *         this.val = val;
 *         this.left = left;
 *         this.right = right;
 *     }
 * }
 */
class Solution {
    public int maxDepth(TreeNode root) {
       
       if(root==null)
        return 0;

        Queue<TreeNode> que=new LinkedList<TreeNode>();
        TreeNode cur=root;
        que.offer(cur);
        int n=0;
        

        while(!que.isEmpty()){
            int len=que.size();
            
            while(len>0){
                
                cur=que.poll();
                
                if(cur.left!=null)
                que.offer(cur.left);
                if(cur.right!=null)
                que.offer(cur.right);
                len--;
            }
            n++;   
        }
        return n;

        
    }
}
